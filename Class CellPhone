class CellPhone{
    public void ring(){
        System.out.println("Ringing...");
    }
    public void vibrate(){
        System.out.println("Vibrating...");
    }
    public void callFriend(){
        System.out.println("Calling Mom");
    }
}
 class methods{
    public static void main(String args[]){
        CellPhone vivo = new CellPhone();
        vivo.callFriend();
        vivo.ring();
        vivo.vibrate();
    }
}
