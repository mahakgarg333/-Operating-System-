class MyEmployee{
    private int id;
    private String name;

    public String getName(){
        return name;
    }
    public void setName(String n){
        this.name = n;
    }
    public void setId(int i){
        this.id = i;
    }
    public int getId(){
        return id;
    }
}
public class Getters{
    public static void main(String args[]){
        MyEmployee mahak = new MyEmployee();
        mahak.setName("MahakGarg");
        System.out.println(mahak.getName());
        mahak.setId(333);
        System.out.println(mahak.getId());
     }
    }
